{
  "name": "pokeranking-api",
  "version": "1.0.0",
  "description": "Pokeranking API",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "DEBUG=* tsx watch src/index.ts",
    "build": "tsup src",
    "lint": "eslint --ext .ts",
    "test": "ENVIRONMENT=test jest",
    "test:integration": "ENVIRONMENT=test jest -c jest.config.integration.ts",
    "prepare": "cd .. && husky install api/.husky",
    "pipeline": "npm ci; npm run lint; npm run build; npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielborgesdm/pokeranking.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "mongo"
  ],
  "author": "Gabriel Borges",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabrielborgesdm/pokeranking/issues"
  },
  "homepage": "https://github.com/gabrielborgesdm/pokeranking#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/i18n": "^0.13.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "i18n": "^0.15.1",
    "mongoose": "^7.0.3",
    "nodemailer": "^6.9.1",
    "zod": "^3.21.4"
  }
}
